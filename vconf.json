{
    "base_box": "ubuntu/xenial64",
    "vb_props": {
        "vm_name": "vagrant_ubuntu_1604_metect-prodopt",
        "cpu_count": "4",
        "cpu_cap_p": "70",
        "ram_amount": "4096"
    },

    "prov_base": "./vconf/packages/",
    "prov_scripts": [
        "system_update.sh",
        "install_git.sh",
        "install_freetds.sh",
        "install_pip3.sh"
    ],

    "env_mod_path": "/etc/profile.d/env_mod.sh",
    "env_mod_cmds": [
        "cd /vagrant",
        "mv ubuntu*.log log/"
    ],

    "make_exec": [
        "startup.sh",
        "run_tests.sh",
        "flask_init.py"
    ],

    "forward_ports": {
        "guest": "5000",
        "host": "5000"
    }
}
